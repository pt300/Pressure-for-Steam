"resource/layout/overlay_communityhubpanel.layout" {
	colors {
		lightestBlue="137 235 255 255"
	}

	styles {
		button {
			textcolor=blue
			font-size=15
			font-size=14 [$LINUX]
			font-family=basefont
			inset-left=0
			padding-left=24
			padding-top=0
			padding-bottom=15
			render_bg {
				1="image( x0, y0, x0 + 26, y1, graphics/overlay/communityhub )"
			}
		}

		button:hover {
			textcolor=lightestBlue
			render_bg {
				1="image( x0, y0, x0 + 26, y1, graphics/overlay/communityhub_hover )"
			}
		}

		DarkRegion {}
		PreviewImage {}
		highlight {
			textcolor=lightestGrey
			font-size=14
		}
	}

	layout {
		region {
			name=box
			width=max
			height=max
		}

		place {
			controls=ViewCommunityHub
			region=box
			x=0
			y=0
			width=205
		}

		place {
			controls=NumContributionsText
			start=ViewCommunityHub
			dir=down
			x=30
			y=-14
		}

		place {
			control="TitleText"
			width=0
			height=0
		}
	}
}