"resource/layout/musiclibrarypanel.layout" {
	styles {
		CMusicLibraryPanel {
			bgcolor=none
			minimum-width=332
			minimum-height=308
			render_bg {
				0="gradient( x0, y0, x1, y0+275, grey, lightgreyend)"
				3="fill( x0, y0 + 275, x1, y1, lightgreyend )"
			}
		}

		LibrarySearch {
			textcolor="darkestGrey"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			inset-left=0
			padding-left=4
			padding-top=1
			//padding-right=20
			minimum-width=156
			render_bg {
				1="image(x0-19, y0, x0+3, y0+25, graphics/search/left)"
				2="image_tiled(x0+3, y0, x1-14, y0+24, graphics/search/inner)"
				3="image(x1-14, y0, x1-2, y0+25, graphics/search/right)"
			}
		}

		LibrarySearch:empty {
			textcolor="lightestGrey"
			font-family=basefont
			font-style=italic
			padding-left=4
		}

		LibrarySearch:hover {
			textcolor="darkGreyEnd"
			padding-left=4
		}

		LibrarySearch:empty:hover {
			textcolor="lightGreyEnd"
			font-style=italic
			padding-left=4
		}

		LibrarySearch:disabled {
			textcolor="None"
			font-family=basefont
			font-style=italic
			padding-left=4
		}

		LibrarySearchIcon {
			bgcolor="none"
			inset="10 0 0 0"
			image=""
			render_bg{}
		}

		LibrarySearchIcon:disabled {
			bgcolor="none"
			inset="10 0 0 0"
			image=""
			render_bg{}
		}

		MusicAddButton {
			textcolor="labelfocus"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-style=normal
			render_bg {
				0="image( x0, y0 + 5, x1, y1, graphics/icon_collapse )"
			}
		}

		MusicAddButton:hover {
			textcolor="White"
			render_bg {
				0="image( x0, y0 + 5, x1, y1, graphics/icon_collapse_over )"
			}
		}

		MusicPlayerIcon {
			bgcolor="none"
			padding-left=-4
			padding-top=-1
			padding-right=-1
			inset="-1 3 0 0"
			image="graphics/icon_music_player"
			render_bg{}
		}

		MusicPlayerIcon:hover {
			bgcolor="none"
			padding-left=-4
			padding-top=-1
			padding-right=-1
			inset="-1 3 0 0"
			image="graphics/icon_music_player_hover"
			render_bg={}
		}

		MusicPlayerIcon:selected {
			bgcolor="none"
			padding-left=-4
			padding-top=-1
			padding-right=-1
			inset="-1 3 0 0"
			image="graphics/icon_music_player_selected"
			render_bg={}
		}

		NavLabel2 [!$OSX] {
			textcolor="labelfocus"
			font-family=basefont
			font-size=14
			font-weight=400
			font-style="uppercase"
			inset="0 2 0 0"
		}

		NavLabel2 [$OSX] {
			textcolor="labelfocus"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			font-style="uppercase"
			inset="0 2 0 0"
		}

		NavLabel2:hover {
			textcolor="texthover"
			font-style="outerglow,uppercase"
			font-outerglow-color="TextGlowHover"
			font-outerglow-offset=1
			font-outerglow-filtersize=3
			inset="0 2 0 0"
		}

		NavLabel2:selected {
			textcolor="White"
			font-style="outerglow,uppercase"
			font-outerglow-color="TextGlowHover"
			font-outerglow-offset=1
			font-outerglow-filtersize=3
			inset="0 2 0 0"
		}

		grouper {
			bgcolor=none
			render_bg {
				0="fill( x0 + 2, y0 + 1, x1 - 10, y1 - 1, none )"
			}
		}
	}

	layout {
		region {
			name=searchrow
			x=8
			y=40
		}

		region {
			name=details
			y=75
			width=max
		}

		place {
			control="subnavgroup_library"
			align=top-center
			y=30
			width=max
			height=28
			margin-left=10
			margin-right=1
		}

		place {
			control=label_search_icon
			region=searchrow
			x=0
			y=0
			width=0
			height=0
		}

		place {
			control=library_search
			region=searchrow
			x=18
			y=0
			width=156
		}

		place {
			control=library_music_menu,library_music_add_button
			region=searchrow
			dir=left
			start=library_search
			margin-left=2
			y=0
		}

		place {
			control="library_music_player_icon"
			region=searchrow
			align=right
			x=4
			y=3
			width=18
			height=18
			margin-right=10
		}

		place {
			control="MusicPage_Details_Content"
			region=details
			x=0
			y=0
			width=max
			height=max
			margin-right=0
			margin-bottom=0
		}
	}
}