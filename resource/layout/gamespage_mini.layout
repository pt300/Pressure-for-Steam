		"CGamesPage_Mini CMiniGameFilterMenuButton" {
			render_bg {
				// background fill
				0="gradient( x0 + 1, y0 + 1, x1 - 1, y1 - 2, grey, lightGreyEnd )" // Background Gradient

				// Dark Borders around
